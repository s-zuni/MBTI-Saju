{
    "version": 2,
    "env": {
        "SUPABASE_URL": "@SUPABASE_URL",
        "SUPABASE_ANON_KEY": "@SUPABASE_ANON_KEY",
        "OPENAI_API_KEY": "@OPENAI_API_KEY"
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/((?!api|static|favicon.ico|logo.*|manifest.json|robots.txt).*)",
            "destination": "/index.html"
        }
    ]
}